<%- include('partials/header'); -%>
<center>

<video
    id="my-video"
    class="video-js"
    controls
    preload="auto"
    width="720"
    height="480"
    poster="<%= video.vidThumb %>"
    data-setup="{}"
  >
    <source src="<%= video.vidLink %>" type="video/mp4" />
    <p class="vjs-no-js">
      To view this video please enable JavaScript, and consider upgrading to a
      web browser that
      <a href="https://videojs.com/html5-video-support/" target="_blank"
        >supports HTML5 video</a
      >
    </p>
  </video>
  </center>
  <form class="lk" action="/like/<%= video._id %>" method="get" style="padding-left: 280px">
    <button type="submit" class ="btn btn-primary"name="button"><i class="fa-solid fa-heart"></i></button>
  </form>

<script src="https://kit.fontawesome.com/231bca5e25.js" crossorigin="anonymous"></script>
<script src="https://vjs.zencdn.net/8.0.4/video.min.js"></script>
<%- include('partials/footer'); -%>
